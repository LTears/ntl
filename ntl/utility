#pragma once

#include "type_traits"

namespace ntl {

    template<typename T>
    typename remove_reference<T>::type&& move(T &&t) {
        return static_cast<typename remove_reference<T>::type&&>(t);
    }

    template<typename T>
    void swap(T &a, T &b) {
        T t = move(a);
        a = move(b);
        b = move(t);
    }

}
